{% extends "_layouts/react" %}
{% includeJsFile "/dist/js/forgotpassword.bundle.js" %}
{% includeCssFile "/dist/css/forgotpassword.bundle.css" %}

{% set token %}
    window.csrfTokenValue = "{{ craft.app.request.csrfToken }}";
    {% if errors is defined %}
    window.errors = "{{ errors|json_encode }}";
    {% endif %}
{% endset %}
{% includeJs token first %}
