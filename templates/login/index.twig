{% extends "_layouts/react" %}
{% includeJsFile "/dist/js/login.bundle.js" %}
{% includeCssFile "/dist/css/login.bundle.css" %}

{% set token %}
    window.csrfTokenValue = "{{ craft.app.request.csrfToken }}";
    {% if errorMessage is defined %}
    window.errors = "{{ errorMessage }}";
    {% endif %}
{% endset %}
{% includeJs token first %}
